<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.bikenmaharjan.w6_p1_2.MainActivity">


    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.bikenmaharjan.w6_p1_2.top_partFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout="@layout/top_part_fragment" />

    <fragment
        android:id="@+id/fragment3"
        android:name="com.example.bikenmaharjan.w6_p1_2.BottonPartFragment"
        android:layout_width="wrap_content"
        android:layout_height="280dp"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment2"
        app:layout_constraintVertical_bias="0.63"
        tools:layout="@layout/botton_part_fragment" />
</android.support.constraint.ConstraintLayout>
